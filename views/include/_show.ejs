<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].status %></td>   
        <td><%= users[i].address %></td>
        <td><%= users[i].cname %></td>   
        <td><%= users[i].phnumber %></td>
        <td><%= users[i].cl %></td>
        <td >
            <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update nk">
                <span class="text-gradient w3-large"><i class="fas fa-pencil-alt nk"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
                <span class="text-gradient w3-large"><i class="fas fa-times nk"></i></span>
            </a>
        </td>
        
    </tr>
    <% } %>

   
    